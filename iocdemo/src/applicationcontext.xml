<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns:context="http://www.springframework.org/schema/context"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans 
	   		http://www.springframework.org/schema/beans/spring-beans.xsd
	   			http://www.springframework.org/schema/context 
	   		http://www.springframework.org/schema/context/spring-context.xsd">
					
 	<bean id="bankSecurityService" class="com.openv.book.springframework40.ioc.BankSecurityServiceImpl">
 	</bean>
 
	<bean id="bankSecurityDao" class="com.openv.book.springframework40.ioc.BankSecurityDaoImpl" primary="true">
	</bean>

	<bean id="cmbBankSecurityDao" class="com.openv.book.springframework40.ioc.BankSecurityDaoImpl">
		<qualifier value="cmb"/>
	</bean>

	<bean id="abcBankSecurityDao" class="com.openv.book.springframework40.ioc.BankSecurityDaoImpl">
		<qualifier value="abc"/>
	</bean>

	<context:annotation-config/>
	
</beans>
